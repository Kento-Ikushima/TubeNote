<h1>タスク一覧ページ</h1>
  
    <% @tasks.each do |task| %>
      <iframe
          src="https://www.youtube.com/embed/<%= task.url.last(11)%>" 
          frameborder="0" 
          allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" 
          allowfullscreen
      >
      </iframe>
      <%= task.description %>
    <% end %>
