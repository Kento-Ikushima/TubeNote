<h1>タスク編集</h1>
<%= form_with model: @mytask, url: my_task_path do |f| %>

  <div><%= f.label :url %></div>       
  <div><%= f.text_field :url %></div>  
  <div><%= f.label :description %></div>       
  <div><%= f.text_field :description %></div> 
  <div><%= f.label :folder_id %></div>        
  <div><%= f.text_field :folder_id %></div>  

  <div><%= f.submit %></div>    
  
  
          #ここにフォルダ選択プルタブを作りたい  
<% end %>
<%= link_to '戻る', my_tasks_path %>