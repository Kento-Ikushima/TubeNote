<h1 class="text-center">New Folder</h1>
<div class="wrapper text-center">
  <%= render 'shared/nav' %>
  <%= form_for(@folder) do |f| %>
    <div><%= f.label :Folder_name %></div>       
    <div><%= f.text_field :folder_name %></div>  
    <div><%= f.label :status %></div>       
    <div><%= f.select :status, [["release", "release"], ["close", "close"]] %></div>  
    <div><%= f.submit"Submit" %></div>             
  <% end %>
</div>